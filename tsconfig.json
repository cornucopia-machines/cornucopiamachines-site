{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "rootDir": ".",
    "types": ["node"],
    "resolveJsonModule": true
  },
  "include": ["_data/**/*.ts"],
  "exclude": ["node_modules", "_site", "_build"]
}
